{
  "name": "statistics-service",
  "version": "1.0.0",
  "description": "Statistics service for query event aggregation",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "inspect-queue": "bash scripts/inspect-queue.sh",
    "produce": "bash scripts/produce-messages.sh",
    "consume": "bash scripts/consume-messages.sh",
    "logs": "bash scripts/check-service-logs.sh"
  },
  "dependencies": {
    "@types/amqplib": "^0.10.8",
    "amqplib": "^0.10.9",
    "express": "^4.21.2",
    "node-cron": "^3.0.3",
    "pg": "^8.13.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.19.0",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.11.10",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^4.0.6"
  }
}
